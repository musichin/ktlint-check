{
    "env": {
        "node": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 12
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "quotes": [1, "single"],
        "indent": [2, 2, {"SwitchCase": 1}],
        "max-len": ["error", { "code": 120 }],
        "require-jsdoc" : 0,
        "operator-linebreak": [
            "error",
            "after",
            {
              "overrides": {
                "?": "before",
                ":": "before"
              }
            }
          ]
    }
}
